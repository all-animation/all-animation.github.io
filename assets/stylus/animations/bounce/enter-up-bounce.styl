	0% {
		-moz-transform:translateY(220px);  opacity: 0;
	}50%{
		-moz-transform:translateY(0px); 
	}70%{-moz-transform:translateY(20px); }
	
	100%{
		-moz-transform:translateY(0);  opacity: 1;
	}
}
