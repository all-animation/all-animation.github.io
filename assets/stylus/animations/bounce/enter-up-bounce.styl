	0% {
		-o-transform:translateY(220px);  opacity: 0;
	}50%{
		-o-transform:translateY(0px); 
	}70%{-o-transform:translateY(20px); }
	
	100%{
		-o-transform:translateY(0);  opacity: 1;
	}
}
