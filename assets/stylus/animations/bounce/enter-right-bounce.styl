	0% {
		-moz-transform:translateX(220px);  opacity: 0;
	}50%{
		-moz-transform:translateX(0px); 
	}70%{-moz-transform:translateX(8px); }
	
	100%{
		-moz-transform:translateX(0);  opacity: 1;
	}
}
