@keyframes enterRightBounce {
	0% {
		transform:translateX(220px);  opacity: 0;
	}50%{
		transform:translateX(0px); 
	}70%{transform:translateX(8px); }
	
	100%{
		transform:translateX(0);  opacity: 1;
	}
}
