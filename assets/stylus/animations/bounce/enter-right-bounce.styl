	0% {
		-o-transform:translateX(220px);  opacity: 0;
	}50%{
		-o-transform:translateX(0px); 
	}70%{-o-transform:translateX(8px); }
	
	100%{
		-o-transform:translateX(0);  opacity: 1;
	}
