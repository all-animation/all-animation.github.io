	0% {
		-ms-transform:translateX(220px);  opacity: 0;
	}50%{
		-ms-transform:translateX(0px); 
	}70%{-ms-transform:translateX(8px); }
	
	100%{
		-ms-transform:translateX(0);  opacity: 1;
	}
